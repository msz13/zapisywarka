{
  "sourceRoot": "libs/web/catalog/catalog-feature/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/web/catalog/catalog-feature/**/*.ts"]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/web/catalog/catalog-feature"],
      "options": {
        "jestConfig": "libs/web/catalog/catalog-feature/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "storybook": {
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "uiFramework": "@storybook/angular",
        "port": 4400,
        "config": {
          "configFolder": "libs/web/catalog/catalog-feature/.storybook"
        },
        "projectBuildConfig": "web-catalog-catalog-feature:build-storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nrwl/storybook:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "uiFramework": "@storybook/angular",
        "outputPath": "dist/storybook/catalog-feature-catalog",
        "config": {
          "configFolder": "libs/web/catalog/catalog-feature/.storybook"
        },
        "projectBuildConfig": "web-catalog-catalog-feature:build-storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  },
  "tags": []
}
