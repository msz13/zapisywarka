{
  "projectType": "library",
  "root": "libs/offers/feature-offer-creation",
  "sourceRoot": "libs/offers/feature-offer-creation/src",
  "prefix": "zapisywarka-client-aps",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/offers/feature-offer-creation/src/**/*.ts",
          "libs/offers/feature-offer-creation/src/**/*.html"
        ]
      },
      "styles": [
        "node_modules/bootstrap/dist/css/bootstrap.css",
        "apps/zapisywarka-back-office/src/styles.scss"
      ],
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/offers/feature-offer-creation"],
      "options": {
        "jestConfig": "libs/offers/feature-offer-creation/jest.config.js",
        "passWithNoTests": true
      }
    },
    "storybook": {
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "uiFramework": "@storybook/angular",
        "port": 4400,
        "config": {
          "configFolder": "libs/offers/feature-offer-creation/.storybook"
        }
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nrwl/storybook:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "uiFramework": "@storybook/angular",
        "outputPath": "dist/storybook/offers-feature-offer-creation",
        "config": {
          "configFolder": "libs/offers/feature-offer-creation/.storybook"
        }
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  },
  "tags": []
}
