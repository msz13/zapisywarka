      ],
      "@zapisywarka-client-aps/offers/domain": [
        "libs/offers/domain/src/index.ts"
      ],
      "@zapisywarka-client-aps/catalog/domain-category": [
        "libs/catalog/domain-category/src/index.ts"
      ],
      "@zapisywarka-client-aps/catalog/api": ["libs/catalog/api/src/index.ts"],
      "@zapisywarka-client-aps/catalog/utills/testing": [
        "libs/catalog/utills/testing/src/index.ts"
