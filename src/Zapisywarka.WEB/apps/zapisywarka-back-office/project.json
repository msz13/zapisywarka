{
  "projectType": "application",
  "root": "apps/zapisywarka-back-office",
  "sourceRoot": "apps/zapisywarka-back-office/src",
  "prefix": "zapisywarka-client-aps",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "outputPath": "dist/apps/zapisywarka-back-office",
        "index": "apps/zapisywarka-back-office/src/index.html",
        "main": "apps/zapisywarka-back-office/src/main.ts",
        "polyfills": "apps/zapisywarka-back-office/src/polyfills.ts",
        "tsConfig": "apps/zapisywarka-back-office/tsconfig.app.json",
        "assets": [
          "apps/zapisywarka-back-office/src/favicon.ico",
          "apps/zapisywarka-back-office/src/assets"
        ],
        "styles": [
          "node_modules/bootstrap/dist/css/bootstrap.css",
          "apps/zapisywarka-back-office/src/styles.scss",
          "node_modules/@nebular/theme/styles/prebuilt/default.css"
        ],
        "scripts": [],
        "stylePreprocessorOptions": {
          "includePaths": ["libs/shared/theme/src/lib/styles"]
        },
        "vendorChunk": true,
        "extractLicenses": false,
        "buildOptimizer": false,
        "sourceMap": true,
        "optimization": false,
        "namedChunks": true
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/zapisywarka-back-office/src/environments/environment.ts",
              "with": "apps/zapisywarka-back-office/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": true,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "9mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "70kb",
              "maximumError": "100kb"
            }
          ]
        }
      },
      "defaultConfiguration": ""
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "options": {
        "browserTarget": "zapisywarka-back-office:build"
      },
      "configurations": {
        "production": {
          "browserTarget": "zapisywarka-back-office:build:production"
        }
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "zapisywarka-back-office:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/zapisywarka-back-office/src/**/*.ts",
          "apps/zapisywarka-back-office/src/**/*.html"
        ]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/zapisywarka-back-office"],
      "options": {
        "jestConfig": "apps/zapisywarka-back-office/jest.config.js",
        "passWithNoTests": true
      }
    },
    "storybook": {
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "uiFramework": "@storybook/angular",
        "port": 4400,
        "config": {
          "configFolder": "apps/zapisywarka-back-office/.storybook"
        }
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nrwl/storybook:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "uiFramework": "@storybook/angular",
        "outputPath": "dist/storybook/zapisywarka-back-office",
        "config": {
          "configFolder": "apps/zapisywarka-back-office/.storybook"
        }
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  },
  "tags": []
}
