{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "zapisywarka-rejestracja",
  "generators": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress",
      "strict": true
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "strict": true
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "api-common-infrastructure": "libs/api/common/infrastructure",
    "api-identity-identity-api": "libs/api/identity/identity-api",
    "api-identity-identity-core": "libs/api/identity/identity-core",
    "api-identity-identity-core-test": "libs/api/identity/identity-core-test",
    "catalog-domain-catalog-item": "libs/catalog/domain-catalog-item",
    "catalog-domain-category": "libs/catalog/domain-category",
    "catalog-feature-catalog": "libs/catalog/feature-catalog",
    "catalog-utills-testing": "libs/catalog/utills/testing",
    "db-migrations": "apps/db-migrations",
    "offers-domain": "libs/offers/domain",
    "shared-domain": "libs/shared/domain",
    "shared-material": "libs/shared/material",
    "shared-theme": "libs/shared/theme",
    "shared-ui": "libs/shared/ui",
    "test-libs-web-sign-up-feature-dep-e2e": "apps/test/libs/web/sign-up-feature-dep-e2e",
    "web-identity-domain": "libs/web/identity/domain",
    "web-identity-login-feature": "libs/web/identity/login-feature",
    "web-identity-login-feature-e2e": "libs/web/identity/login-feature-e2e",
    "web-identity-sign-up-feature": "libs/web/identity/sign-up-feature",
    "web-identity-sign-up-feature-dep": "libs/web/identity/sign-up-feature-dep",
    "web-identity-utills": "libs/web/identity/utills",
    "web-landing-page": "libs/web/landing-page",
    "zapisywarka-api": "apps/zapisywarka-api",
    "zapisywarka-api-test": "apps/zapisywarka-api-test",
    "zapisywarka-rejestracja": "apps/zapisywarka-rejestracja",
    "zapisywarka-rejestracja-e2e": "apps/zapisywarka-rejestracja-e2e"
  }
}
